/* dist/w-validate/index.wxss */
.wuss-validate {
  position: relative;
}
.validate-icon {
  display: none;
  position: absolute;
  right: 15rpx;
  top: 50%;
  transform: translate3d(0, -50%, 0);
}
.wuss-validate-showIcon {
  padding-right: 30rpx;
}
.wuss-validate-showIcon .validate-icon {
  display: block;
}
