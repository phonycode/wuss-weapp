<w-mask-shadow
 wx:if="{{ mask }}"
 visible="{{ maskShadow }}"
 maskCancel="{{ maskCancel }}"
 bind:onClose="handleClose"
 zIndex="{{ maskIndex }}"
/>

<view class="wuss-class animate-content animate-content-{{ position }} {{ visible ? 'animate-content-open' : ''}}" style="{{ popupStyles }} {{ styles }}">
	<view class="popup-content">
		<slot/>
	</view>
</view>

